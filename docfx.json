{
  "metadata": [
    {
      "src": [
        {
          "cwd": "repos/",
          "files": [
            "wpilib/src/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ], 
      
      "dest": "api",
      "filter": "filter.yml",
      "properties": {
                "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "tutorials/**.md",
          "tutorials/**/toc.yml",
          "examples/**.md",
          "examples/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "readme.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "content/msdn.4.5.2.zip"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "globalMetadata": {
      "_appTitle": "RobotDotNet",
      "_appFooter" : "Copyright Â© 2017 RobotDotNet"
    }
  }
}
